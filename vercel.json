{
    "version":2,
    "builds":[
        {
            "src":"p1sisworkshop/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"15mb",
                "runtime":"python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/staticfiles-build",
            "config": {
              "distDir": "static_build"
            }
        }
    ],
    "routes":[
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src":"/(.*)",
            "dest":"p1sisworkshop/wsgi.py"
        }
    ]
}
